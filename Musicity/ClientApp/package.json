{
  "name": "musicity",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "prestart": "npm run build-css",
    "start": "react-scripts start",
    "prebuild": "npm run build-minify-css",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.ts ./src/**/*.tsx",
    "build-css": "npx tailwindcss -i ./src/assets/styles/Styles.css -o ./src/assets/styles/Styles.Compiled.css",
    "build-minify-css": "npx tailwindcss -i ./src/assets/styles/Styles.css -o ./src/assets/styles/Styles.Compiled.css --minify",
    "watch-css": "npx tailwindcss -i ./src/assets/styles/Styles.css -o ./src/assets/styles/Styles.Compiled.css --watch"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "antd": "^4.18.8",
    "axios": "^0.26.1",
    "history": "^5.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-google-login": "^5.2.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "svgo": "^2.8.0",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3"
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-google-recaptcha": "^2.1.3",
    "@types/react-helmet": "^6.1.5",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "autoprefixer": "^10.4.4",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "nan": "^2.14.1",
    "node": "^17.4.0",
    "react-scripts": "^5.0.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
